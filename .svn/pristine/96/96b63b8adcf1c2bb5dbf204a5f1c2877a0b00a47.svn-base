<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:padding="20dp"
    android:orientation="vertical"
    android:layout_height="match_parent">


    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Unidade de Medida Temporal"
        android:layout_weight="0"
        android:id="@+id/textView2" />
    <Spinner
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:id="@+id/spinnerTemporal"
        android:layout_weight="0" />
    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Unidade de Medida VolumÃ©trica"
        android:layout_weight="0"
        android:id="@+id/textViewMedidaVolumetrica" />
    <Spinner
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:id="@+id/spinnerVolume"
        android:layout_weight="0" />

    <Button
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="Confirmar"
        android:id="@+id/buttonCalcularGotejamentoTempo"
        android:layout_gravity="center_horizontal"
        android:layout_weight="0"
        android:background="@drawable/sl_button"
        style="@style/style_button" />


</LinearLayout>
