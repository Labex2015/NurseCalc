public class Mililitro implements  UnidadeMedidaVolumeContract {

    public static final int GOTAS = 20;
    static double      LITRO =  0.001;
    public static final String LABEL = "Mililitro";
    public static final String LABELABREVIACAO = "ml";
    public static final double MILILITRO = 1;
    double value;

    @Override
    public double getMililitro(double value) {
        return value * MILILITRO;
    }

    public double getLitro(double value){
        return value * LITRO;
    }
    public double getGotas(double value){
           return value * GOTAS;
    }



}
