<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nursecalc" >

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/launcher"
        android:label="@string/app_name"
        android:theme="@style/CustomActionBarTheme" >
        <activity
            android:name=".Splash"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".Panel"
            android:label="@string/title_activity_panel" >
        </activity>
        <activity
        android:name=".Creditos"
        android:label="@string/title_activity_creditos" >
        </activity>
        <activity
            android:name=".CalcularGotejamento"
            android:label="@string/title_activity_calcular_gotejamento"
            android:parentActivityName="com.nursecalc.Panel">
        </activity>
        <activity
            android:name=".CalcularDosagem"
            android:label="@string/title_activity_calcular_dosagem"
            android:parentActivityName="com.nursecalc.Panel">
        </activity>
        <activity
            android:name=".Formula"
            android:label="@string/title_activity_calcular_gotejamento"
            android:parentActivityName="com.nursecalc.LeXml"
        >
        </activity>
        <activity
            android:name=".LeXml"
            android:label="Escalas"
            android:parentActivityName="com.nursecalc.Panel">
        </activity>
        <activity
            android:screenOrientation="portrait"
            android:name=".EscalaActivity"
            android:label="@string/title_activity_escala"
            android:parentActivityName="com.nursecalc.LeXml">
        </activity>
    </application>

</manifest>
